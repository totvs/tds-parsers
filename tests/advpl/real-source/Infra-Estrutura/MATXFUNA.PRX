	Local lRet:=.f.
	Local cVar
	Local cAlias := Alias( )
	Local nSizeFil := 2

	//-- Atualiza o conteúdo da filial
	If FindFunction("FWSizeFilial")
		nSizeFil := FWSizeFilial()
	EndIf

