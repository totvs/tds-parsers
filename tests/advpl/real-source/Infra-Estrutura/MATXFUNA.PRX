	EndIf
	If lPrjCni
		If SE1->E1_RATFIN == "1"
			Aadd(aBut040,{'DESTINOS',{|| F641AltRat("FINA040",2) },STR0052,STR0053}) //"Visualizar Rateio Financeiro" - "Rat.Financ."
		EndIf

	Else
