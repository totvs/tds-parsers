	Function MSGetAnswer(hPort, cStrs, nTempo)
		Local cResult;

		If PORT_OLD <> hPort
			MSSetPort(hPort)
			PORT_OLD = hPort
		End If
		cResult := ""
		MsClearBuffer(hPort)
		MSSEndStr(cStrs)
		nSec := seconds() + nTempo

		While .T.
			If (seconds() > nSec)
				exit
			End If

			sysRefresh()

			MSRead(hPort, @cResult)

			If !(cResult == "")
				exit
			End If
		End


		Return (cResult)

