	Function A360Tipo(cVar)

		Local nIPI, nSOLID, nTIPO
		Local lRelease	:= GetRpoRelease() >= "R7"
		Local oMdl 		:= FWModelActive()
		Local oMdlSE4

		If lRelease .AND. ValType(oMdl) == "O"

			oMdlSE4 := oMdl:GetModel("SE4MASTER")

			If oMdlSE4:GetValue("E4_TIPO") == "9" .And. (((oMdlSE4:GetIdField("E4_IPI") > 0 .AND. oMdlSE4:GetValue("E4_IPI") $ "JS") .Or. (oMdlSE4:GetIdField("E4_SOLID") > 0 .AND. oMdlSE4:GetValue("E4_SOLID") $ "JS")) .And. !"%"$ oMdlSE4:GetValue("E4_COND")) .Or.;
					(((oMdlSE4:GetIdField("E4_IPI") > 0 .AND. oMdlSE4:GetValue("E4_IPI") $ "S") .Or. (oMdlSE4:GetIdField("E4_SOLID") > 0 .AND. oMdlSE4:GetValue("E4_SOLID") $ "S")) .And. "%" $ oMdlSE4:GetValue("E4_COND") )
				Help( " ",1,"A360TIPO9" )
				If cVar == "T"
					Return .T.
				EndIf

				Return .F.
			ElseIf oMdlSE4:GetValue("E4_TIPO") == "8" .And. oMdlSE4:GetIdField("E4_SOLID") > 0 .AND. oMdlSE4:GetValue("E4_SOLID") $ "JS"

				Help( " ",1,"A360TIPO8" )
				If cVar == "T"
					Return .T.
				EndIf

				Return .F.
			EndIf

		Else
			If M->E4_TIPO == "9" .And. ((M->E4_IPI $ "JS" .Or. M->E4_SOLID $ "JS") .And. !"%"$M->E4_COND) .Or.;
					((M->E4_IPI $ "S" .Or. M->E4_SOLID $ "S") .And. "%"$M->E4_COND)

				Help( " ",1,"A360TIPO9" )
				If cVar == "T"
					nTIPO := Ascan(aGets,{ |x| Subs(x,9,7) == "E4_TIPO" } )
					If nTIPO > 0
						aTela[Val(Subs(aGets[nTIPO],1,2))][Val(Subs(aGets[nTIPO],3,1))*2] :=  M->E4_TIPO
					EndIf
					nIPI := Ascan(aGets,{ |x| Subs(x,9,6) == "E4_IPI" } )
					If nIPI > 0
						M->E4_IPI := Space(Len(E4_IPI))
						aTela[Val(Subs(aGets[nIPI],1,2))][Val(Subs(aGets[nIPI],3,1))*2] :=  M->E4_IPI
					EndIf
					nSOLID := Ascan(aGets,{ |x| Subs(x,9,8) == "E4_SOLID" } )
					If nSOLID > 0
						M->E4_SOLID := Space(Len(E4_SOLID))
						aTela[Val(Subs(aGets[nSOLID],1,2))][Val(Subs(aGets[nSOLID],3,1))*2] := M->E4_SOLID
					EndIf
					lRefresh := .T.
					Return .T.
				EndIf
				Return .F.
			ElseIf M->E4_TIPO == "8" .And. M->E4_SOLID $ "JS"
				Help( " ",1,"A360TIPO8" )
				If cVar == "T"
					nTIPO := Ascan(aGets,{ |x| Subs(x,9,7) == "E4_TIPO" } )
					If nTIPO > 0
						aTela[Val(Subs(aGets[nTIPO],1,2))][Val(Subs(aGets[nTIPO],3,1))*2] :=  M->E4_TIPO
					EndIf
					nSOLID := Ascan(aGets,{ |x| Subs(x,9,8) == "E4_SOLID" } )
					If nSOLID > 0
						M->E4_SOLID := Space(Len(E4_SOLID))
						aTela[Val(Subs(aGets[nSOLID],1,2))][Val(Subs(aGets[nSOLID],3,1))*2] := M->E4_SOLID
					EndIf
					lRefresh := .T.
					Return .T.
				EndIf
				Return .F.
			EndIf
		EndIf

		Return .T.

