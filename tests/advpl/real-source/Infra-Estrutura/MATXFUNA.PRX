Function EanDigito( cCodigo )
	Local j
	Local nMultiplicador
	Local nSoma := 0
	Local nResultado := 0
	Local nDigito
	cCodigo := AllTrim( cCodigo )
	For j:=1 To 12    // Somatoria do conteudo do campo
		nMultiplicador := Iif( ( j % 2) == 0 , 3, 1 ) // varia entre 3 e 1
		nSoma += Int(Val(SubStr(cCodigo,j,1))) * nMultiplicador
	Next j
	nResultado := Int(nSoma / 10 ) + Iif(( nSoma % 10 ) > 0, 1 , 0 )  // Prox Multiplo 10
	nDigito   := nResultado * 10 - nSoma
	Return Str( nDigito , 1 )
