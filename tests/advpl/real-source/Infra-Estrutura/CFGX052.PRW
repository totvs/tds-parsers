	*/
Function CFG052AtuACols(aCampos)
	Local nX         := 0    //Loop em aCampos
	Local nY         := 0    //Loop em aHeader
	Local nPos       := 0    //Posicao do campo dentro do aCols
	Local nPosCpo    := gdFieldPos( "XO_CAMPO" )
	Local nPosOper   := gdFieldPos( "XO_OPER" )
	Local nLenHead   := Len( aHeader )
	Local nPosPesq   := 0    //Pesquisa do campo aCols
	Local nPosGrava  := 0    //Posicao para gravar registro dentro do aCols

	If Empty( aCampos )
		Return
	EndIf

	For nX := 1 to Len( aCampos )
		nPosPesq := aScan( oGetDados:aCols,{| ExpA1 | PadR(ExpA1[nPosCpo],10) == PadR(aCampos[nX][1],10) })
		If nPosPesq <= 0 .Or. aTail( oGetDados:aCols[nPosPesq] )

			If nPosPesq <= 0
				nPosGrava := aScan( oGetDados:aCols,{| ExpA1 | PadR(ExpA1[nPosCpo],10) == Space(10) })

				If nPosGrava <= 0
					AAdd( oGetDados:aCols, Array( nLenHead + 1 ) )
					nPosGrava := Len( oGetDados:aCols )
				EndIf
			Else
				nPosGrava := nPosPesq
			EndIf
			oGetDados:aCols[nPosGrava][nPosCpo]    := aCampos[nX][1]
			oGetDados:aCols[nPosGrava][nPosOper]   := 0
			oGetDados:aCols[nPosGrava][nPosOper]   += IIf( aCampos[nX][2],64,0 )
			oGetDados:aCols[nPosGrava][nPosOper]   += IIf( aCampos[nX][3],128,0 )
			oGetDados:aCols[nPosGrava][nPosOper]   += IIf( aCampos[nX][4],256,0 )
			oGetDados:aCols[nPosGrava][nLenHead+1] := .F.
		EndIf
	Next nX

	oGetDados:Refresh()

	Return

