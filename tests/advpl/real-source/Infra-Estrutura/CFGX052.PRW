	dbSelectArea( "SXO" )
	dbSetOrder( 1 )
	If dbSeek( cTabela )
		MsgStop( STR0021 , STR0017 )//"Jแ existe registros relacionados a tabela selecionada."//"Aten็ใo"
		Return .F.
	EndIf

	oTabela:Disable()

