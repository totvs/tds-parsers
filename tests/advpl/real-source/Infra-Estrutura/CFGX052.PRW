	ฑฑบ          ณ                                                            บฑฑ
	ฑฑฬออออออออออุออออออออออออออออออออออออออออออออออออออออออออออออออออออออออออนฑฑ
	ฑฑบUso       ณ CFGX052                                                    บฑฑ
	ฑฑศออออออออออฯออออออออออออออออออออออออออออออออออออออออออออออออออออออออออออผฑฑ
	ฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑ
	฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿฿
	*/
Function CFG052Man(cAlias, nRecno, nOpc, oExplorer, oListBox)
	Local oDlg                                 //Dialog principal
	Local oPanel                               //Panel Principal
	Local aSize     := MsAdvSize()             //Size da tela.
	Local aObjects  := {}                      //Objetos da tela
	Local aPosObj   := {}                      //Posicoes do objeto
	Local aInfo     := {}                      //Posicoes do objeto
	Local aCriaCols := {}                      //Array que recebe o retorno da funcao a610criacols
	Local oTabela                              //Objeto da Tabela (GET)
	Local aButtons  := {}                      //Array com os botoes para enchoicebar
	Local lOk       := .F.                     //Confirmacao da atualizacao dos dados
	Local aCols     := {}                      //Colunas do msgetdados

	Private cTabela := Space(3)                //Tabela do sistema (GET)
	Private aHeader := {}                      //Header do msgetdados
	Private aColsR  := {}                      //Recno dos dados da CriaCols
	Private oGetDados                          //Objeto da MsGetDados

	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณ Adiciona o botao de Estrutura do arquivo.                               ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	AAdd(aButtons,{"NOVACELULA"  ,{|| IIf(Str(nOpc,1)$"2/5",Nil,CFG052AtuACols(aClone( CFG052List(cTabela)) ))},STR0011} )//"Consultar Estrutura..."
	AAdd(aButtons,{"BMPCONS"     ,{|| IIf(Str(nOpc,1)$"2/5",Nil,GdSeek(oGetDados)) },STR0012} )//"Pesquisar..."
	AAdd(aButtons,{"LBOK"        ,{|| IIf(Str(nOpc,1)$"2/5",Nil,CFG052TpLog()) },STR0013} )//"Selecionar tipo de LOG..."

	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณ Carrega variaveis de memoria.                                           ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	dbSelectArea( "SXO" )
	RegToMemory( "SXO", ( nOpc == 3 ) )

	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณ Montagem do Header.                                                     ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	AAdd(aHeader,{"Campo"   ,"XO_CAMPO","@!",010,0 ,"CFG052VldCpo()" ,"๛" ,"C"," "," ",NIL,,,,})
	AAdd(aHeader,{"Operacao","XO_OPER" ,"999",003,0 ,".T." ,"๛" ,"N"," "," ",NIL,,,"V",})

	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณ Monta aCols do sistema.                                                 ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	dbSelectArea( "SXO" )
	dbSetOrder( 1 )
	If nOpc == 3
		aCriaCols := aClone( CFG052Cria("SXO", aHeader, "   ", {|| .F.}) )
	Else
		cTabela := SXO->XO_ALIAS
		aCriaCols := aClone( CFG052Cria("SXO", aHeader, SXO->XO_ALIAS,{|| SXO->XO_ALIAS == cTabela }) )
	EndIf

	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณ Faco a distribuicao dos dados de retorno da CFG052Cria                  ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	aCols  := aClone( aCriaCols[1] )
	aColsR := aClone( aCriaCols[2] )

	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณ Define as posicoes da GetDados e Paineis.                               ณ
