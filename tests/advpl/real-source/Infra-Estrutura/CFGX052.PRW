Function CFGX052
	Local oExplorer                     //Objeto Explorer
	Local oListBox                      //ListBox do explores
	Local aResource   := {}             //Painel do Resource
	Local nI          := 0              //Contador do sistema
	Local nTop        := 01             //Posicao acima da dialog
	Local nLeft       := 01             //Posicao da esquerda do dialog
	Local nBottom	  := 0              //posicao abaixo do dialog
	Local nRight	  := 0              //posicao da direita do dialog
	Local oPanels     := {}             //variavel contendo os objetos paineis em uma array
	Local nPanel      := 0              //Qtde de paineis na array

	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณ Abertura do arquivo SXO                                                ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	dbSelectArea( "SXO" )
	dbSetOrder( 1 )
	dbGotop()

	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณ Objeto explorer.                                                       ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	oExplorer := MSExplorer():New( STR0001 )//"Configuracao de LOG de campos"
	oExplorer:DefaultBar()

	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณCriacao da Arvore          - Nivel 0 ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	Aadd( oPanels, oExplorer:AddTree(STR0002,"BMPPARAM",,PadR("CFG052Man",20)) )//"Campos para LOG"
	nPanel := Len( oPanels )

	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณCalculo do Explorer para varios pixeis do windows                       ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	nBottom	  := Int((oExplorer:aPanel[nPanel]:nHeight * .99))
	nRight	  := Int((oExplorer:aPanel[nPanel]:nWidth * .49))

	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณ Objeto ListBox aberto diretamente a base de dados SXO                  ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	@ nTop, nLeft LISTBOX oListBox FIELDS SXO->XO_ALIAS,;
		SXO->XO_CAMPO,;
		SXO->XO_OPER ;
		HEADER STR0003,;//"Alias"
	STR0004,;//"Campo"
	STR0005 ;//"Ordem"
	ALIAS  "SXO" ;
		SIZE   nRight,(nBottom * 0.5) ;
		OF     oExplorer:GetPanel(nPanel) PIXEL

	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณMonta o conjunto de botoes da tool bar do objeto Explorer e as respectivas chamadas das funcoesณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	oExplorer:AddSearchButton({|cSearch,lSCase,lSWord| CFG052Pesq(cSearch,lSCase,lSWord)})
	AAdd( aResource, {"FINAL"     , STR0006 , {|| oExplorer:Deactivate()}})//"Sair"
	AAdd( aResource, {"BMPINCLUIR", STR0007 , {|| CFG052Man("SXO",SXO->(Recno()),DF_INCLUIR,oExplorer,oListBox)} })//"Incluir"
	AAdd( aResource, {"BMPVISUAL" , STR0008 , {|| CFG052Man("SXO",SXO->(Recno()),DF_VISUAL,oExplorer,oListBox)}  })//"Visualizar"
	AAdd( aResource, {"EDIT"      , STR0009 , {|| CFG052Man("SXO",SXO->(Recno()),DF_ALTERAR,oExplorer,oListBox)} })//"Alterar"
	AAdd( aResource, {"EXCLUIR"   , STR0010 , {|| CFG052Man("SXO",SXO->(Recno()),DF_EXCLUIR,oExplorer,oListBox)} })//"Excluir"

	For nI := 1 To Len(aResource)
		oExplorer:AddDefButton( aResource[nI,1], aResource[nI,2], aResource[nI,3] )
	Next nI

	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณ Ativa objeto explorer.                                                 ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	oExplorer:Activate(.T.,{|| .T. })

	Return

	/*
	
	ฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑฑ
	ฑฑษออออออออออัออออออออออหอออออออัออออออออออออออออออออหออออออัอออออออออออออปฑฑ
	ฑฑบPrograma  ณCFG052Man บAutor  ณFernando Salvatori  บ Data ณ 21/01/2004  บฑฑ
	ฑฑฬออออออออออุออออออออออสอออออออฯออออออออออออออออออออสออออออฯอออออออออออออนฑฑ
