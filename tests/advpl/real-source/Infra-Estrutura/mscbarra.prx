Function MsAvalKey(oBrowse,nTecla,lGetInativo,lExit,aListBox)

    Do Case
    Case nTecla==K_UP 	; oBrowse:Up()
    Case nTecla==K_DOWN 	; oBrowse:Down()
    Case nTecla==K_RIGHT ; oBrowse:Right()
    Case nTecla==K_LEFT 	; oBrowse:Left()
    Case nTecla==K_TAB 	; lGetInativo:=.F.
    Case nTecla==K_F5		; MsGetQuant(oBrowse)
    Case nTecla==K_ENTER ; MsGetQuant(oBrowse)
    Case nTecla==K_F6 	; MsApagaItm(oBrowse,aListBox)
    Case nTecla==K_ESC 	; lExit:=.T.
    EndCase

Return
