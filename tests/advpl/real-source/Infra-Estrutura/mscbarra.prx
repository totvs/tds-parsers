Function GetSX3Info(cCampo)

    Local aInfo := { "","",0,0 }

    Local cArea := Alias(),;
        nOrd1 := IndexOrd(),;
        nRecN := RecNo()

    Local nOrd2 := 0

    DbSelectArea("SX3"); nOrd2 := IndexOrd(); DbSetOrder(2)

    If DbSeek(cCampo,.F.)
        aInfo[1] := X3Titulo()
        aInfo[2] := SX3->X3_TIPO
        aInfo[3] := SX3->X3_TAMANHO
        aInfo[4] := SX3->X3_DECIMAL
    EndIf

    DbSetOrder(1)

    DbSelectArea(cArea); DbSetOrder(nOrd1); DbGoTo(nRecN)

Return(aInfo)
