	#ENDIF

	Local aCampos := {{"EMP1" 	,"C", 2,0},;
		{"FIL1" 		,"C", 2,0},;
		{"CONTA1"	,"C",20,0},;
		{"CUSTO1"	,"C", 9,0},;
		{"MOEDA1"	,"C", 5,0},;
		{"CRITER1"	,"C", 4,0},;
		{"EMP2"   	,"C", 2,0},;
		{"FIL2" 		,"C", 2,0},;
		{"CONTA2"	,"C",20,0},;
		{"CUSTO2"	,"C", 9,0},;
		{"MOEDA2"	,"C", 5,0},;
		{"CRITER2"	,"C", 4,0}}

	Local cCad, cCad1, cCad2

