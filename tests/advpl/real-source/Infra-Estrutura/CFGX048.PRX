	/*/
STATIC Function CF048Skip(nPula)
	LOCAL nCnt := 0

	If nPula > 0
		While nCnt < nPula
			nPosBco++
			Skip 1
			If Eof()
				nPosBco--
				Skip -1
				Exit
			EndIf
			nCnt++
		EndDo
	Else
		While nCnt > nPula
			nPosBco--
			Skip -1
			If Bof()
				nPosBco++
				Exit
			Endif
			nCnt--
		EndDo
	EndIf

	Return nCnt

