Function SngUpdat()

	DbSelectArea("SN1")
	DbSeek(xFilial())
	While ! Eof()
		If ! Empty(N1_GRUPO)
			DbSelectArea("SNG")
			If ! DbSeek(xFilial() + SN1->N1_GRUPO)
				RecLock("SNG", .T.)
				Replace NG_FILIAL 	With xFilial(),;
					NG_GRUPO 	With SN1->N1_GRUPO,;
					NG_DESCRIC	With SN1->N1_GRUPO
				MsUnLock()
			Endif
			DbSelectArea("SN1")
		Endif
		DbSkip()
	EndDo

	Return .T.