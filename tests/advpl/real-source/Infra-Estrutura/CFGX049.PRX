				If (!SubStr(aCols[n,1],2,1) $ "HDT" .Or.;
						!If(SubStr(aCols[n,1],2,1)$"HT",SubStr(aCols[n,1],3,1)==" ",.T.) )
					Help(" ",1,"X049LINOK")
					lRetorno := .F.
				EndIf
			EndIf
			oGet:Refresh()
			Return(lRetorno)

