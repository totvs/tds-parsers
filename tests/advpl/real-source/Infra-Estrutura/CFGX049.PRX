				For nCntFor1 := 1 To Len(aTemp)
					If (aTemp[nCntFor1,1]!=cIdent)
						aadd(aDetalhe,aTemp[nCntFor1])
					EndIf
				Next
				For nCntFor1 := 1 To Len(aCols)
					aadd(aDetalhe,aCols[nCntFor1])
				Next
			Endif

			aCols   := aClone(aSavAcols)
			aHeader := aClone(aSavaHeader)
			N       := nSavN
