			/*/

		Function x049TudOk(oGet)

			Local lRetorno:= .T.
			Local nCntFor := 0
			Local cIdent  := ""
			aSort(aCols,,,{|x,y|x[1] <= y[1] })
			For nCntFor := 1 To Len(aCols)
				If ( !aCols[nCntFor,Len(aCols[nCntFor])] )
					If ( cIdent == aCols[nCntfor,1] )
						Help(" ",1,"X049TUDOK")
						lRetorno := .F.
					EndIf
					cIdent := aCols[nCntfor,1]
				EndIf
			Next nCntFor
			oGet:Refresh()
			Return(lRetorno)
